pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                withCredentials([string(credentialsId: 'my-jenkins-api-token', variable: 'JENKINS_TOKEN')]) {
                    sh 'curl -u ghddhksduq:${JENKINS_TOKEN} http://10.0.2.246:8080/job/chatbot/build?token=my-trigger-token'
                }
            }
        }
    }
}
